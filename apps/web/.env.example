# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_DEMO_MODE=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_USE_SHARE_ID=true
NEXT_PUBLIC_ENABLE_OPTIMIZE=true
NEXT_PUBLIC_ENABLE_AI=true

# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
# NEXT_PUBLIC_GOOGLE_MAP_ID=your_map_id_here  # Uncomment and add Map ID to enable AdvancedMarkerElement

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Backend API Configuration
TRAILWRIGHT_API_URL=https://your-api-domain.com

# AI Provider Configuration
AI_PROVIDER=openai
# Use either ANTHROPIC_API_KEY or OPENAI_API_KEY based on AI_PROVIDER
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Optional - Stripe for payments (future feature)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Optional - PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key_here
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# FastAPI Service URL
NEXT_PUBLIC_API_URL=http://localhost:8000

# Testing Configuration
TEST_DATABASE_URL=postgresql://postgres:password@localhost:54322/postgres
TEST_SUPABASE_URL=http://localhost:54321
TEST_SUPABASE_ANON_KEY=test_anon_key
TEST_SUPABASE_SERVICE_KEY=test_service_key